pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: 'main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Constance2021/ci_with_docker.git']]])
            }
        }
        stage('Build') {
            steps {
                sh 'docker buid -t jenk-image'
            }
        }
    }
}
